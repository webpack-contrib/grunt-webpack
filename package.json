{
  "name": "grunt-webpack",
  "description": "Use webpack with grunt.",
  "version": "2.0.0-beta.5",
  "homepage": "https://github.com/webpack/grunt-webpack",
  "author": {
    "name": "Tobias Koppers @sokra"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:webpack/grunt-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/webpack/grunt-webpack/issues"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "src",
    "tasks"
  ],
  "peerDependencies": {
    "webpack": "^2.1.0-beta || ^2.0.0 || ^2.2.0-rc.2"
  },
  "license": "MIT",
  "keywords": [
    "gruntplugin"
  ],
  "scripts": {
    "coverage": "nyc report --reporter=json && codecov -f coverage/coverage-final.json",
    "lint": "eslint src tasks",
    "preversion": "npm test",
    "test": "npm run lint && npm run test-only",
    "test-ci": "nyc npm run test-only",
    "test-only": "ava"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.9",
    "babel-preset-latest": "^6.16.0",
    "codecov": "^1.0.1",
    "eslint": "^3.6.1",
    "fs-extra": "^1.0.0",
    "glob": "^7.0.5",
    "grunt": "^1.0.0",
    "nyc": "^10.0.0",
    "webpack": "^2.1.0-beta"
  },
  "dependencies": {
    "lodash": "^4.7.0"
  },
  "ava": {
    "files": [
      "**/tests/**/*.js",
      "!**/utils/**/*",
      "!**/fixtures/**/*",
      "!**/tmp/**/*"
    ]
  },
  "nyc": {
    "include": [
      "src/",
      "tasks/"
    ]
  }
}
